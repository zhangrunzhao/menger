
.navCard{
	background-color: #FFFFFF;
	position: fixed;
	top: 0;
	width: 100%;
	z-index: 999;
	-webkit-box-shadow: 0 0 10rpx rgba(0, 0, 0, 0.1);
	        box-shadow: 0 0 10rpx rgba(0, 0, 0, 0.1);
}
.titleNav{
	height: 138rpx;
	padding-top: var(--status-bar-height);
}
.Swiper{
	margin-top: 95px;
}
.title{
	height: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
	padding: 0 40rpx;
	padding-top: 15rpx;
}
.search,.title-ico,.title-name,.title-circle{
	font-size: 1.3em;
	font-weight: 800;
}
.title-ico,.search{
	padding-top: 9rpx;
	font-size: 1.5em;
	font-weight: 200;
}
.title-ico{
}
.title-name{
	color: #E6B8FC;
}
.title-circle{
	margin-left: 22rpx;
	color: #E6B8FC;
}
.onCho{
	border-bottom: 2px solid #000;
	color: #000;
}
.searchBox{
	position: relative;
}
.search{
	color: #E6B8FC;
	position: absolute;
	right: 20rpx;
	top: 0;
}
.searchInput{
	width: 380rpx;
	height: 60rpx;
	padding-left: 30rpx;
	border-radius: 30rpx;
	background-color: rgba(0,0,0,0.1);
}
.myCircle{
	font-size: 35rpx;
	font-weight: 600;
	padding-left: 30rpx;
}
.myCirecle_list{
	display: block;
	position: relative;
	top: 20rpx;
	width: 100%;
	height: 100%;
}
.myCirecle_box{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	height: 145rpx;
	padding-bottom: 10rpx;
	padding-top: 10rpx;
	padding-left: 30rpx;
	border-top: 2rpx solid #E8E8E8;
}
.myCirecle_box:active{
	background-color: #FAFAFA;
}
.myCircleBox_Boximg{
	height: 115rpx;
	width: 115rpx;
	padding-right: 20rpx;
}
.myCircleBox_img{
	height: 100%;
	text-align: center;
}
.myCirecle_nameBox{
	width: 70%;
	font-size: 30rpx;
}
.time{
	position: relative;
	font-size: 20rpx;
}
.myCirecle_name1{
	padding-top: 5rpx;
	padding-bottom: 15rpx;
	font-weight: 700;
}
.myCirecle_name2{
	color: #D6D6D6;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 1;
	text-align: justify;
	overflow: hidden;
}
.middle{
	position: relative;
	top:150rpx
}

